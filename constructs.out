   (balance (dia ?top) (mes ?mes_top) (ano ?ano_top))
   (partida (numero ?numero) (dia ?dia) (mes ?mes) (ano ?ano))
   (test (>= (to_serial_date ?top ?mes_top ?ano_top) (to_serial_date ?dia ?mes ?ano)))
