   (subtotales (cuenta salarios) (deber ?salarios))
   (subtotales (cuenta gastos-ventas) (deber ?gastos-ventas))
   (subtotales (cuenta gastos-en-investigacion-y-desarrollo) (debe ?gastos-en-investigacion-y-desarrollo))
